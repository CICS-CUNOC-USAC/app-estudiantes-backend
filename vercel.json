{
  "buildCommand": "npx knex --knexfile ./dist/knexfile.js migrate:latest && npx knex --knexfile ./dist/knexfile.js seed:run",
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ]
}